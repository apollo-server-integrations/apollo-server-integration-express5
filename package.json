{
  "name": "@as-integrations/express5",
  "description": "An Apollo Server integration for use with Express v5",
  "version": "1.1.2",
  "author": "Apollo <packages@apollographql.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/apollo-server-integrations/apollo-server-integration-express5"
  },
  "homepage": "https://github.com/apollo-server-integrations/apollo-server-integration-express5",
  "bugs": {
    "url": "https://github.com/apollo-server-integrations/apollo-server-integration-express5/issues"
  },
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/cjs/index.d.ts",
        "default": "./dist/esm/index.d.ts"
      },
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "compile": "tsc --build tsconfig.build.json",
    "postcompile": "echo '{\"type\":\"module\"}' > dist/esm/package.json && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "clean": "git clean -dfqX",
    "prepack": "npm run compile",
    "prettier-check": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "changeset-publish": "npm run compile && changeset publish",
    "changeset-check": "changeset status --verbose --since=origin/main",
    "changeset-version": "changeset version && npm i",
    "spell-check": "cspell lint '**' --no-progress || (echo 'Add any real words to cspell-dict.txt.'; exit 1)",
    "test": "jest",
    "test:ci": "jest --coverage --ci --maxWorkers=2 --reporters=default --reporters=jest-junit",
    "watch": "tsc --build --watch",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@apollo/server": "4.12.2",
    "@apollo/server-integration-testsuite": "4.12.2",
    "@apollo/utils.withrequired": "3.0.0",
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.6",
    "@eslint/js": "9.34.0",
    "@types/compression": "1.8.1",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/express-serve-static-core": "5.0.7",
    "@types/jest": "29.5.14",
    "@types/node": "20.19.11",
    "@types/supertest": "6.0.3",
    "compression": "1.8.1",
    "cors": "2.8.5",
    "cspell": "9.2.0",
    "eslint": "9.34.0",
    "express": "5.1.0",
    "graphql": "16.11.0",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "prettier": "3.6.2",
    "supertest": "7.1.4",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "typescript-eslint": "8.40.0"
  },
  "peerDependencies": {
    "@apollo/server": "^4.0.0 || ^5.0.0",
    "express": "^5.0.0"
  }
}
